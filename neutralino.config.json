{
  "applicationId": "js.neutralino.sample",
  "cli": {
    "binaryName": "rom-viewer",
    "binaryVersion": "4.7.0",
    "clientLibrary": "/src/vendor/neutralino.js",
    "clientVersion": "3.6.0",
    "extensionsPath": "/extensions/",
    "resourcesPath": "/src/"
  },
  "defaultMode": "window",
  "documentRoot": "/src/",
  "enableNativeAPI": true,
  "enableServer": true,
  "globalVariables": {},
  "logging": {
    "enabled": false,
    "writeToLogFile": false
  },
  "modes": {
    "browser": {
      "globalVariables": {},
      "nativeBlockList": ["filesystem.*"]
    },
    "chrome": {
      "args": "--user-agent=\"Neutralinojs chrome mode\"",
      "height": 960,
      "nativeBlockList": ["filesystem.*", "os.*"],
      "width": 1280
    },
    "cloud": {
      "nativeAllowList": ["app.*"],
      "url": "/src/#cloud"
    },
    "window": {
      "alwaysOnTop": false,
      "borderless": false,
      "enableInspector": false,
      "exitProcessOnClose": true,
      "fullScreen": false,
      "height": 960,
      "hidden": false,
      "icon": "/src/assets/icons/dvd_32px.png",
      "maximize": true,
      "minHeight": 540,
      "minWidth": 672,
      "resizable": true,
      "title": "ROM Viewer",
      "width": 1280
    }
  },
  "nativeAllowList": ["app.*", "os.*", "filesystem.*", "debug.log"],
  "port": 0,
  "tokenSecurity": "one-time",
  "url": "/",
  "version": "1.0.6"
}
